pipeline {
    agent none
    
    stages {
    	stage('Example Build') {
		parallel {
		agent {docker 'maven:3.8.1-adoptopenjdk-11'}
		steps {
			echo 'Hello, Maven'
			sh 'mvn --version'
		}
		}
		
	}
	
	stage('Example Test') {
		agent{ docker 'node:17-alpine3.14'}
		steps {
			echo 'NPM Version'
			sh 'npm --version'
		}
	}
    }
}
